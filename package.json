{
  "name": "defer-relay",
  "version": "1.0.0",
  "description": "Defer/Stream with Relay Example App",
  "type": "module",
  "scripts": {
    "relay": "relay-compiler",
    "format": "prettier --write .",
    "parcel": "parcel ./client/index.html --open",
    "server": "node --watch --loader ts-node/esm server/server.ts",
    "build": "relay-compiler && tsc && parcel build ./client/index.html",
    "start": "concurrently --prefix-colors yellow,blue,magenta,cyan -n server,client,relay \"npm run server\" \"npm run parcel\" \"npm run relay -- --watch\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/react-relay": "^18.2.0",
    "@types/relay-runtime": "^18.2.5",
    "babel-plugin-relay": "^16.1.0",
    "concurrently": "^8.2.2",
    "parcel": "^2.11.0",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "relay-compiler": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "fetch-multipart-graphql": "^3.2.1",
    "graphql": "^17.0.0-alpha.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^18.2.0",
    "relay-runtime": "^18.2.0"
  },
  "resolutions": {
    "graphql": "^17.0.0-alpha.9"
  },
  "prettier": {}
}
